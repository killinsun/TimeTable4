<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.sample.time_table"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <uses-sdk android:minSdkVersion="10" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name=".TimeTableActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="TimeTableEditActivity"></activity>
        <activity android:name="TimeTableListActivity" android:label="@string/app_name"></activity>
        <activity android:name="TimeTableDayOfWeekActivity" android:label="@string/app_name"></activity>
        <activity android:name="TimeTableDetaileActivity" android:label="@string/app_name"></activity>
        <activity android:name="TimeTableList" android:label="@string/app_name"></activity>
        <activity android:name="AddVarietyActivity"></activity>
        <activity android:name="PrefsActivity"></activity>
    </application>

</manifest>